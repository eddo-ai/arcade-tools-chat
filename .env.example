# Arcade API Configuration
ARCADE_API_KEY=your_arcade_api_key_here

# User ID for Arcade API (typically your email address)
USER_ID=your_user_id_here

# Model Configuration
# Use models that support tool calling like gpt-4o or claude-3-opus-20240229
MODEL=gpt-4

# OpenAI Fallback Configuration (Optional)
OPENAI_API_KEY=your_openai_api_key_here  # Only needed if you want to use OpenAI fallback

# Optional: Streamlit Configuration
STREAMLIT_SERVER_PORT=8501