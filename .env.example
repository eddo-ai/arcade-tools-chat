# This file shows the required environment variables for local development
# For deployment, these should be added to .streamlit/secrets.toml instead

# Arcade API Configuration
ARCADE_API_KEY=your-arcade-api-key

# User ID for Arcade API (typically your email address)
USER_ID=your_user_id_here

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4

# Optional: Streamlit Configuration
STREAMLIT_SERVER_PORT=8501

# LangSmith Configuration
LANGSMITH_API_KEY=your-langsmith-api-key
LANGCHAIN_TRACING_V2=true
LANGSMITH_PROJECT=your-project-name
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Optional: LangSmith project settings
# LANGSMITH_PROJECT=your-project-name  # The project name to use in LangSmith
# LANGSMITH_ENDPOINT=https://api.smith.langchain.com  # The LangSmith API endpoint

# Example secrets.toml structure:
#
# [default]
# OPENAI_API_KEY = "your-openai-api-key"
# OPENAI_MODEL = "gpt-4"
# ARCADE_API_KEY = "your-arcade-api-key"
# LANGSMITH_API_KEY = "your-langsmith-api-key"
# LANGCHAIN_TRACING_V2 = "true"
# LANGSMITH_PROJECT = "your-project-name"
# LANGSMITH_ENDPOINT = "https://api.smith.langchain.com"